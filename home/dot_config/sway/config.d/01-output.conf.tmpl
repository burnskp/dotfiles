# Framework 13 2.8k Matte Screen
output * scale 2
output * color_profile icc ../icc/NE135FBM-NY1.icc

{{ if eq .theme "light" -}}
output * bg ~/.config/sway/wallpaper/flatppuccin_4k_latte_mauve.png fill
{{ else -}}
output * bg ~/.config/sway/wallpaper/fw_cobalt2.png fill
{{ end }}
exec brightnessctl -q s 32%
